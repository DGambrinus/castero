create table feed (
    key             text primary key,
    title           text,
    description     text,
    link            text,
    last_build_date text,
    copyright       text
);

create table episode (
    id          integer primary key,
    title       text,
    feed        text,
    description text,
    link        text,
    pubdate     text,
    copyright   text,
    enclosure   text
);